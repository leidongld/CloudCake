<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.openld.cloudcake">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CloudCakeApplication"
        android:allowBackup="false"
        android:icon="@drawable/icon_app"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/icon_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.CloudCake"
        tools:ignore="AllowBackup,Instantiatable,UnusedAttribute">
        <activity android:name=".cityselect.v.CitySelectActivity" />
        <activity android:name=".main.v.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>